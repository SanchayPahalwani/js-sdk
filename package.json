{
  "name": "openpay_sdk",
  "version": "0.0.1",
  "description": "OpenPay-ID sdk",
  "main": "dist/openpay.min.js",
  "scripts": {
    "exodus": "node_modules/.bin/parcel samples/exodus_gamma.html --https",
    "sample": "node_modules/.bin/parcel samples/sdk_test.html --https",
    "blockstack": "node_modules/.bin/parcel samples/blockstack.html --https",
    "build:js": "node_modules/.bin/parcel build src/index.ts -d dist --out-file openpay.min.js",
    "build:dom": "node_modules/.bin/parcel build --no-minify src/openpay-dom.ts -d dist --out-file openpay-dom.js",
    "build": "npm run build:js && npm run build:dom",
    "dist-server": "python -m SimpleHTTPServer 8777"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "parcel": "^1.12.3",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@mojotech/json-type-validation": "^3.1.0",
    "bitcoinjs-lib": "^5.1.4",
    "blockstack": "^19.2.5",
    "eciesjs": "^0.2.0",
    "eventemitter3": "^4.0.0",
    "js-logger": "^1.6.0",
    "jsontokens": "^2.0.2",
    "peerjs": "git+ssh://git@bitbucket.org:coinswitch/webrtc-client.git",
    "regenerator-runtime": "^0.13.3"
  }
}
