{
  "name": "@coinswitch/openpay",
  "version": "0.0.28",
  "description": "OpenPay SDK",
  "unpkg": "dist/openpay-dom.js",
  "main": "lib/exports.js",
  "types": "lib/exports.d.ts",
  "scripts": {
    "exodus": "./node_modules/.bin/parcel src/samples/exodus_gamma.html --https",
    "sample": "./node_modules/.bin/parcel src/samples/sdk_test.html --https",
    "typecheck": "./node_modules/.bin/tsc --noEmit",
    "transpile": "./node_modules/.bin/tsc || true",
    "build:js": "./node_modules/.bin/parcel build src/index.ts -d dist --out-file openpay.js --no-minify",
    "build:dom": "./node_modules/.bin/parcel build src/openpay-dom.ts -d dist --out-file openpay-dom.js --no-minify",
    "build": "npm run build:js && npm run build:dom",
    "dist-server": "python -m SimpleHTTPServer 8777",
    "prepare": "",
    "publish:dev": "cross-env NODE_ENV=dev npm run build && npm run transpile && gulp config && npm version patch && npm publish --registry http://138.197.216.253:4873",
    "publish:prod": "cross-env NODE_ENV=prod npm run build && npm run transpile && gulp config && npm version patch && npm publish"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^5.2.1",
    "gulp": "^4.0.2",
    "gulp-env": "^0.4.0",
    "gulp-rename": "^1.4.0",
    "parcel-bundler": "^1.12.3",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@mojotech/json-type-validation": "^3.1.0",
    "bitcoinjs-lib": "^5.1.4",
    "blockstack": "^19.2.5",
    "eciesjs": "^0.2.0",
    "eventemitter3": "^4.0.0",
    "js-logger": "^1.6.0",
    "jsontokens": "^2.0.2",
    "peerjs": "^1.0.5",
    "regenerator-runtime": "^0.13.3"
  }
}
